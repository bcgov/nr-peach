{
  "name": "nr-peach",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "description": "NR Permitting Exchange, Aggregation and Collection Hub",
  "author": "NRM Permitting and Data Solutions <NRM.PermittingAndData@gov.bc.ca>",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "codegen": "kysely-codegen --url $(node --experimental-transform-types dburl.helper.ts)",
    "clean": "rimraf coverage",
    "debug": "node --experimental-transform-types --inspect server.ts",
    "dev": "node --experimental-transform-types --watch server.ts",
    "format": "prettier --write \"**/*.{json,md,ts,yaml}\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "migrate:down": "kysely migrate down",
    "migrate:latest": "kysely migrate latest",
    "migrate:list": "kysely migrate list",
    "migrate:make": "kysely migrate make",
    "migrate:rollback": "kysely migrate rollback --all",
    "migrate:up": "kysely migrate up",
    "postcodegen": "npm run format",
    "postformat": "npm run lint:fix",
    "postinstall": "simple-git-hooks",
    "postlint": "npm run typecheck",
    "purge": "rimraf node_modules",
    "seed:list": "kysely seed list",
    "seed:make": "kysely seed make",
    "seed:run": "kysely seed run",
    "start": "cross-env NODE_ENV=production node --experimental-transform-types server.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tsconfig/node22": "22.0.2",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "compression": "1.8.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "express-winston": "4.2.0",
    "helmet": "8.1.0",
    "js-yaml": "4.1.0",
    "kysely": "0.28.2",
    "kysely-codegen": "0.18.5",
    "kysely-ctl": "0.13.1",
    "lru-cache": "11.1.0",
    "pg": "8.16.0",
    "serve-favicon": "2.5.0",
    "winston": "3.17.0",
    "winston-transport": "4.9.0"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.15.31",
    "@types/pg": "8.15.4",
    "@types/serve-favicon": "2.5.7",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.2.2",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-jsdoc": "50.6.11",
    "eslint-plugin-prettier": "5.2.6",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.13.0",
    "supertest": "7.1.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vitest": "3.2.2"
  }
}
