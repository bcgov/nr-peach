{
  "name": "nr-peach",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "description": "NR Permitting Exchange, Aggregation and Collection Hub",
  "author": "NRM Permitting and Data Solutions <NRM.PermittingAndData@gov.bc.ca>",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "clean": "rimraf coverage",
    "debug": "ts-node-dev --debug --respawn --transpile-only --rs --watch bin,config,dist server.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "migrate:down": "kysely migrate down",
    "migrate:latest": "kysely migrate latest",
    "migrate:list": "kysely migrate list",
    "migrate:make": "kysely migrate make",
    "migrate:rollback": "kysely migrate rollback --all",
    "migrate:up": "kysely migrate up",
    "postrebuild": "npm run build",
    "postreinstall": "npm install",
    "prelint": "npm run typecheck",
    "prelint:fix": "npm run typecheck",
    "pretest": "npm run lint",
    "posttest": "node --experimental-transform-types lcov-fix.ts",
    "purge": "rimraf node_modules",
    "rebuild": "npm run clean",
    "reinstall": "npm run purge",
    "seed:list": "kysely seed list",
    "seed:make": "kysely seed make",
    "seed:run": "kysely seed run",
    "serve": "ts-node-dev --respawn --transpile-only --rs --watch bin,config,dist server.ts",
    "start": "node --experimental-transform-types server.ts",
    "test": "jest --verbose --forceExit --detectOpenHandles",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "5.1.0",
    "kysely": "0.27.6",
    "kysely-ctl": "0.12.1",
    "pg": "8.14.1"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@tsconfig/node22": "22.0.0",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/pg": "8.11.11",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.6",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "ts-jest": "29.3.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0"
  }
}
