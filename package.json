{
  "name": "nr-peach",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "description": "NR Permitting Exchange, Aggregation and Collection Hub",
  "author": "NRM Permitting and Data Solutions <NRM.PermittingAndData@gov.bc.ca>",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "clean": "rimraf coverage",
    "debug": "node --experimental-transform-types --inspect server.ts",
    "format": "prettier --write \"**/*.{json,md,ts,yaml}\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "migrate:down": "kysely migrate down",
    "migrate:latest": "kysely migrate latest",
    "migrate:list": "kysely migrate list",
    "migrate:make": "kysely migrate make",
    "migrate:rollback": "kysely migrate rollback --all",
    "migrate:up": "kysely migrate up",
    "postformat": "npm run lint:fix",
    "postinstall": "simple-git-hooks",
    "prelint": "npm run typecheck",
    "prelint:fix": "npm run typecheck",
    "pretest": "npm run lint",
    "pretest:cov": "npm run lint",
    "posttest:cov": "node --experimental-transform-types lcovFix.util.ts",
    "purge": "rimraf node_modules",
    "seed:list": "kysely seed list",
    "seed:make": "kysely seed make",
    "seed:run": "kysely seed run",
    "serve": "node --experimental-transform-types --watch server.ts",
    "start": "node --experimental-transform-types server.ts",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "compression": "1.8.0",
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "express-winston": "4.2.0",
    "helmet": "8.1.0",
    "js-yaml": "4.1.0",
    "kysely": "0.28.1",
    "kysely-ctl": "0.12.2",
    "pg": "8.14.1",
    "serve-favicon": "2.5.0",
    "winston": "3.17.0",
    "winston-transport": "4.9.0"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@tsconfig/node22": "22.0.1",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.14.1",
    "@types/pg": "8.11.13",
    "@types/serve-favicon": "2.5.7",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-jsdoc": "50.6.9",
    "eslint-plugin-prettier": "5.2.6",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.12.1",
    "ts-jest": "29.3.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.0",
    "vitest": "3.1.1"
  }
}
